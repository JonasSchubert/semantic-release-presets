{
  "name": "semantic-release-presets",
  "version": "4.1.3",
  "description": "Semantic release presets and plugins for various projects.",
  "scripts": {
    "prepare": "is-ci || husky"
  },
  "files": [
    "plugins/*",
    "presets/*"
  ],
  "keywords": [
    "semantic-release",
    "semantic-release-plugins",
    "semantic-release-presets",
    "ms-teams",
    "nuget"
  ],
  "homepage": "https://github.com/JonasSchubert/semantic-release-presets",
  "bugs": {
    "url": "https://github.com/JonasSchubert/semantic-release-presets/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JonasSchubert/semantic-release-presets.git"
  },
  "author": {
    "name": "Jonas Schubert",
    "email": "jonas.schubert.projects@web.de",
    "url": "https://github.com/JonasSchubert"
  },
  "contributors": [],
  "license": "MIT",
  "dependencies": {
    "execa": "^9.1.0",
    "got": "^14.2.1",
    "hpagent": "^1.2.0",
    "lodash": "^4.17.21",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "commitlint-presets": "^1.0.5",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "commitlint": {
    "extends": [
      "./node_modules/commitlint-presets/index.js"
    ]
  },
  "release": {
    "extends": "semantic-release-presets/presets/github/.releaserc.full.json"
  }
}
